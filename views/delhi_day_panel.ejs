        integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3" crossorigin="anonymous">
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
        crossorigin="anonymous"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <style>
        .maincell {
            border: 1px solid black;
            width: 70px;
            height: 80px;
            display: flex;
            padding-left: 20px;
            font-weight: 800;
            /* font-size: xx-large; */
        }

        /* span {
            font-size: large;
        } */

        .datecell>p {
            font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
            font-size: 16px;
            margin: 0px;
            font-weight: 400;
            padding-right: 15px;
            display: block;
            width: 80px;
            font-weight: bolder;
            padding-top: 10px;

        }

        .smallnumbersleft>p {
            margin-left: 0px;
            font-size: 14px;
            padding-left: 2px;
        }

        .smallnumbersright>p {
            margin-left: 0px;
            font-size: 14px;
            padding-left: 2px;
        }

        .bignumbercell>p {
            font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
            margin: 0px;
            padding-left: 5px;
            margin-top: 17px;
            font-size: 15px;
            font-weight: bolder;
        }
    </style>
    <style>
        * {
            padding: 0;
            margin: 0;
            box-sizing: border-box;
        }

        body {
            font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
            background-color: rgb(154, 209, 237);
            padding: 0px;

        }

        h3 {
            font-weight: 800;
            font-size: 23px;
            font-family: initial;
        }

        label {
            font-weight: 700;
            font-size: 40px;
            font-weight: 800;
        }

        .time {
            font-size: 23px;
            font-weight: 800;
        }

        .wrapper {
            margin-left: 11%;
            margin-right: 11%;
            background-color: whitesmoke;
            /* width: 70%; */
        }

        @media only screen and (max-width: 700px) {
            .wrapper {
                margin-left: 0%;
                margin-right: 0%;
                background-color: whitesmoke;
                width: 100%;

            }

            .datecell>p {
                font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
                font-size: 10px;
                margin: 0px;
                font-weight: bolder;
                padding-right: 11px;
                display: block;
                width: 80px;
                font-weight: bolder;
                padding-top: 10px;

            }

            .smallnumbersleft>p {
                margin: 0px;
                font-size: 10px;
                padding: 2px;
            }

            .smallnumbersright>p {
                margin: 0px;
                font-size: 10px;
                padding: 2px;
            }

            .bignumbercell>p {
                font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
                margin: 0px;
                font-size: 10px;
                margin-top: 15px;
                font-weight: bolder;
                padding-left: 2px;
            }

            .maincell {
                border: 1px solid black;
                width: 50px;
                height: 50px;
                display: flex;
                padding: 0px;
                font-weight: 800;
            }

            .row_Data {
                padding-top: 0px;
            }


        }

        .row_Data {
            display: flex;
        }

        .s8 {
            border: 1px solid blue;
            height: auto;
            background-color: #f2f2f2;
        }
    </style>

    <style>
        .body {

            height: auto;
            font-family: "Helvetica Neue", Helvetica, Arial, sans-serif;
            font-size: 14px;
            font-weight: 800;
            color: #333;

        }

        .container {
            width: auto;
            background-color: white;
            min-height: 0px !important;
            height: auto !important;
            padding: 0px;
            padding-right: 60px;
            padding-left: 60px;
            margin-top: -1%;
        }

        .s1 {

            background-color: #f4511e;
            color: white;
            border: 1px solid skyblue;
        }

        .s2:hover {
            text-decoration: underline;
            cursor: pointer;
        }

        .s3 {
            background-color: black;
            color: white;
            height: 40px;
            margin-top: -27px;
        }

        .s4 {
            border: 1px solid skyblue;
            height: 250px;
            background-color: white;
        }

        .s5 {
            width: 100%;

        }

        .s6 {
            text-align: center;
        }

        .s7 {
            background-color: #337ab7;
            height: 50px;
            margin-top: -2%;
            padding: 1%;
            font-size: 24px;
            color: white;
            margin-top: 0px;
            line-height: 1.1;

        }

        /* .s8 {
        border: 1px solid blue;
        height: auto;
        background-color: #f2f2f2;
    } */

        .s9 {
            border: 1px solid white;
            height: 500px;
            background-color: white;
        }

        .s10 {
            background-color: #292f38;
            height: 1.5em;
            text-align: center;
            color: white;
            padding: 0.5em;
        }
    </style>


</head>

<body>

    <div class="wrapper" style="font-family: initial;">
        <div style="border: 1px solid; margin-left: 0px;">
            <div style="background-color: #f4511e;">
                <h1 style="text-align: center; font-weight: bold; color: whitesmoke ;">MAINDELHISATTA.COM</h1>
                <p style="text-align: center; font-size: 100%; color: whitesmoke; ">सबसे तेज ➣ सबसे आगे</p>
                <div style="background-color: black;">
                    <marquee style="color: aliceblue;"> Delhi सट्टा मटका की <b> Original </b> वेबसाईट </marquee>
                </div>
            </div>

            <br>

            <div class="">
                <div class="row">
                    <div class="col-md-12">
                        <!-- <div style="background-color: #337ab7; padding: 0%; margin: 0%;" class="panel-heading">
                        <h3>Chennai</h3>
                    </div> -->

                        <div style="background-color: #337ab7; " class="card-heading">
                            <br>
                            <h3 style="text-align: center;color: aliceblue;">MAIN DELHI DAY PANEL CHART </h3>
                            <br>
                            <h1></h1>
                        </div>
                        <!-- <div> -->


                        <% for(var a=0; a < details.length ; a+=7){%>
                            <div class="row_Data" style="border: none;">
                                <div class="datecell" style="border: none; font-style: italic;">

                                    <p>
                                        <%= details[a].date %>
                                    </p>

                                </div>
                                <% for(var i=a,h=0 ; h < 7 && i < details.length ; i++,h++){%>

                                    <div class="maincell" style="border: none; ">
                                        <div class="smallnumbersleft" style="border: none;">
                                            <p>
                                                <% for(var j=0 ; j < 3 ; j++){%>
                                                    <% if(days[i].left==null || days[i].left=="" || days[i].left=="***"
                                                        ){ %>
                                                        <span style="color: red;">*</span><br>
                                                        <% } else{ %>

                                                            <% if(days[i].red==-1 ){ %>
                                                                <span style="color: red;">
                                                                    <%= days[i].left[j]%>
                                                                </span><br>
                                                                <% } else{ %>
                                                                    <span>
                                                                        <%= days[i].left[j]%>
                                                                    </span><br>
                                                                    <% } %>
                                                                        <% } %>

                                                                            <%}%>
                                            </p>
                                        </div>
                                        <div class="bignumbercell">
                                            <% if(details[i].left=="" || details[i].right=="" || details[i].left==null
                                                || details[i].right==null ){ %>
                                                <% if((details[i].left=="" || details[i].left==null) &&(
                                                    details[i].right=="" || details[i].right==null)){ %>
                                                    <p style="color: red;">**
                                                    </P>

                                                    <%} else if( details[i].right=="" || details[i].right==null){ %>
                                                        <p>
                                                            <%=details[i].left%><span style="color: red;">*</span>
                                                        </P>

                                                        <% }else{ %>

                                                            <p><span style="color: red;">*</span>
                                                                <%=details[i].right%>
                                                            </P>
                                                            <% } %>

                                                                <% } else{ %>
                                                                    <% if(days[i].red==-1 ){ %>
                                                                        <p style=" color: red;">
                                                                            <%= details[i].left %>
                                                                                <%= details[i].right %>
                                                                        </P>
                                                                        <% } else{ %>
                                                                            <p>
                                                                                <%= details[i].left %>
                                                                                    <%= details[i].right %>
                                                                            </P>
                                                                            <% } %>
                                                                                <% } %>
                                        </div>
                                        <div class="smallnumbersright">
                                            <p>
                                                <% for(var j=0 ; j < 3 ; j++){%>
                                                    <% if( days[i].right==null ||
                                                        days[i].right==null||days[i].right=="***" ){ %>
                                                        <span style="color:#ff0000">*</span><br>
                                                        <% } else{ %>
                                                            <% if(days[i].red==-1 ){ %>
                                                                <span style="color: red;">
                                                                    <%= days[i].right[j]%>
                                                                </span><br>
                                                                <% } else{ %>
                                                                    <span>
                                                                        <%= days[i].right[j]%>
                                                                    </span><br>
                                                                    <% } %>
                                                                        <% } %>
                                                                            <%}%>
                                            </p>

                                        </div>


                                    </div>
                                    <%}%>
                            </div>
                            <%}%>





                                <!-- </div> -->
                    </div>

                </div>
            </div>



        </div>

    </div>

    <br>
    <div style="background-color: black; text-align: center;">
        <span style="color: whitesmoke;"> Copyright</span>
        <a style="color: yellow;" href="#">@maindelhimataka.com</a>
    </div>



</body>
<script>
    $(window).resize(function () {
        if ($(window).width() < 800); {
            $('.wrapper').removeClass('wrapper');
        }
    });
</script>
